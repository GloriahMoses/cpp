#include <iostream>
#include <string>
#include<cmath>
using namespace std;

int addition(int num[]){
    int sum = 0;
    for(int i = 0; i<5; i++){
        sum = sum + num[i];
    }
    return sum;
    
}

int main()
{
  int num[5], count, total;
  cout <<"Enter five numbers:" ;
  
  for (count = 0; count < 5; count = count+1){
      cin>>num[count];
  }
  
  total = addition(num);
  cout <<"Total:" <<total;
  
  return 0;
}
