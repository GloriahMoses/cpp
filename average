#include <iostream>
#include <string>
#include<cmath>
using namespace std;

int findaverage(int num[]){
    int sum = 0;
    
    for(int i = 0; i<10; i++){
        sum = sum + num[i];
    }
    return sum/10;
    
}

int main()
{
  int num[10], count, total;
  cout <<"Enter ten numbers:";
  int average;
  
  for (count = 0; count < 10; count = count+1){
      cin>>num[count];
  }
  average = findaverage(num);
  cout <<"Average:" <<average;
  
  return 0;
}
